- if @class_template.class_template_class_types.any?
  %ul.list-group
    - @class_template.class_template_class_types.each do |class_template_class_type|
      %li.list-group-item
        .row
          .col-xs-9= link_to class_template_class_type.clazz_type.name, edit_admin_class_type_path(class_template_class_type.clazz_type)
          .col-xs-3= link_to "Remove", admin_class_template_class_type_path(class_template_class_type), method: :delete, class: "btn btn-danger"
