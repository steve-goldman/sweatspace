- if @class_template.class_template_studios.any?
  %ul.list-group
    - @class_template.class_template_studios.each do |class_template_studio|
      %li.list-group-item
        .row
          .col-xs-9= link_to class_template_studio.studio.name, edit_admin_studio_path(class_template_studio.studio)
          .col-xs-3= link_to "Remove", admin_class_template_studio_path(class_template_studio), method: :delete, class: "btn btn-danger"
