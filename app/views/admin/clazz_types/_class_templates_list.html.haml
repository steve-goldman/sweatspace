- if @clazz_type.class_template_class_types.any?
  %table.table
    - @clazz_type.class_template_class_types.each do |class_template_class_type|
      %tr
        %td= link_to class_template_class_type.class_template.name, edit_admin_class_template_path(class_template_class_type.class_template)
        %td= link_to "Remove", admin_class_template_class_type_path(class_template_class_type), method: :delete, class: "btn btn-danger"
