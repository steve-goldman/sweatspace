- if @clazz_type.class_template_class_types.any?
  %ul.list-group
    - @clazz_type.class_template_class_types.each do |class_template_class_type|
      %li.list-group-item
        = link_to class_template_class_type.class_template.name, edit_admin_class_template_path(class_template_class_type.class_template)
        = link_to "Remove", admin_class_template_class_type_path(class_template_class_type), method: :delete, class: "btn btn-danger btn-xs pull-right"
