var $searchList = $("#search-list")
$searchList.empty();
<% if @instructor_profiles.any? %>
  <% @instructor_profiles.each do |instructor_profile| %>
    $searchList.append("<%= escape_javascript(render('result_row', instructor_profile: instructor_profile)) %>");
  <% end %>
<% else %>
  $searchList.append("<%= escape_javascript(render('no_results')) %>");
<% end %>
